{
    "name": "<%= appNameSlug %>",
    "description": "<%= appDescription %>",
    "version": "<%= appVersion %>",
    "homepage": "<%= githubUrl %>",
    "author": {
        "name": "<%= authorName %>",
        "email": "<%= authorEmail %>"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/<%= userName %>/<%= appNameSlug %>.git"
    },
    "bugs": {
        "url": "<%= githubUrl %>/issues"
    },
    "licenses": [{
        "type" : "<%= license %>",
        "url" : "<%= githubUrl %>/blob/master/LICENSE"
    }],
    "main": "artifacts/<%= appNameSlug %>.js",
    "scripts": {
        "build": "haxe targets/js.hxml",
        "test": "haxelib run munit test -result-exit-code"
    },
    "dependencies": {
    },
    "devDependencies": {
    },
    "keywords": [
        "slushgenerator"
    ]
}
